<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite parallel="false" name="NopCommerce - UI Automation">
<parameter name = "userUrl" value = "https://demo.nopcommerce.com/"/>
 <parameter name = "adminUrl" value = "https://admin-demo.nopcommerce.com"/> 

<listeners>
      <listener class-name="commons.MethodListener"/>
</listeners>

<test name="Run on Chrome">
  	<parameter name = "browser" value = "Chrome"/>
 	<classes>
      <class name="com.nopcommerce.user.Level_15_Assert_Verify"/>
    </classes>
</test>
  
</suite>
