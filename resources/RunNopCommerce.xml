<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite parallel="false" name="NopCommerce - UI Automation">
<parameter name = "userUrl" value = "https://demo.nopcommerce.com/"/>
 <parameter name = "adminUrl" value = "https://admin-demo.nopcommerce.com"/> 

<test name="Run on Chrome">
  	<parameter name = "browser" value = "Chrome"/>
 	<classes>
      <class name="com.nopcommerce.user.Level_09_Switch_Site_URL"/>
    </classes>
</test>

<test name="Run on Firefox">
  	<parameter name = "browser" value = "Firefox"/>
 	<classes>
      <class name="com.nopcommerce.user.Level_09_Switch_Site_URL"/>
    </classes>
</test>
  
</suite>
